package Test;

abstract class test1 {
    abstract public void show();
}

class test2 extends test1 {
    public void show() {
        System.out.println("In test 2");
    }
}

class test3 extends test1 {
    public void show() {
        System.out.println("In test 3");
    }
}

public class Test {

	public static void main(String[] args) {
		
		test1 t = null;
		
//		t = new test1();
		t.show();
		t = new test2();
		t.show();
		t= new test3();		
		t.show();
	}

}
