package Test;

class test1 {
    public void show() {
        System.out.println("In test 1");
    }
}

class test2 extends test1 {
    public void show() {
        System.out.println("In test 2");
    }
}

class test3 extends test1 {
    public void show() {
        System.out.println("In test 3");
    }
}

public class Test {

	public static void main(String[] args) {
		
		test1 t1 = new test1();
		test1 t2 = new test2();
		test1 t3 = new test3();
		
		t1.show();
		t1.show();
		t1.show();
	}

}
