package Friends;

import java.util.Date;

public class FriendsService {
	
	static int count = 0;
	static Friends[] friarr;
	////id,name,lastname,hobbies,mobno,email,bdate,address
	static {
		friarr = new Friends[10];
		friarr[0] = new Friends(1,"abc", "cba", 1234567890, "abc@gmail.com", new Date(2001,5,13), "Pune");
		friarr[1] = new Friends(2,"def", "fed", 1234567890, "def@gmail.com", new Date(2002,5,25), "Nashik");
		friarr[2] = new Friends(3,"xyz", "zyx", 1234567890, "xyz@gmail.com", new Date(2003,5,8), "Nagpur");
		count = 3;
	}
	
	public static void displayAll() {
		
		for (Friends friends : friarr) {
			System.out.println(friends);
		}
		
	}

}
